{
  "name": "丽心心理咨询中心",
  "version": "1.0.0",
  "description": "聊城市东阿县丽心心理咨询中心，是一家专业心理健康咨询机构。在丽心，我们相信每一颗心都值得被温柔以待。在这里，没有评判，只有理解与陪伴；没有孤独，只有专业引导与温暖支持。",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev --turbopack",
    "dev:prod": "cross-env NODE_OPTIONS=--no-deprecation rm -rf .next && pnpm build && pnpm start -p 1666",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "ii": "cross-env NODE_OPTIONS=--no-deprecation pnpm --ignore-workspace install",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "lint:fix": "cross-env NODE_OPTIONS=--no-deprecation next lint --fix",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "reinstall": "cross-env NODE_OPTIONS=--no-deprecation rm -rf node_modules && rm pnpm-lock.yaml && pnpm --ignore-workspace install",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start -p 1666",
    "start:node": "cross-env NODE_OPTIONS=--no-deprecation cp -r public .next/standalone/ && cp -r .next/static .next/standalone/.next/ && tar zcvf standalone.tgz .next/standalone && PORT=1666 HOSTNAME=0.0.0.0 node .next/standalone/server.js",
    "ui": "pnpm dlx shadcn@latest"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.2",
    "@heroicons/react": "^2.2.0",
    "@payloadcms/admin-bar": "3.38.0",
    "@payloadcms/db-postgres": "3.38.0",
    "@payloadcms/email-nodemailer": "^3.38.0",
    "@payloadcms/email-resend": "3.38.0",
    "@payloadcms/live-preview-react": "3.38.0",
    "@payloadcms/next": "3.38.0",
    "@payloadcms/payload-cloud": "^3.38.0",
    "@payloadcms/plugin-form-builder": "3.38.0",
    "@payloadcms/plugin-nested-docs": "3.38.0",
    "@payloadcms/plugin-redirects": "3.38.0",
    "@payloadcms/plugin-search": "3.38.0",
    "@payloadcms/plugin-seo": "3.38.0",
    "@payloadcms/richtext-lexical": "3.38.0",
    "@payloadcms/translations": "^3.38.0",
    "@payloadcms/ui": "3.38.0",
    "@radix-ui/react-accordion": "^1.2.10",
    "@radix-ui/react-checkbox": "^1.3.1",
    "@radix-ui/react-label": "^2.1.6",
    "@radix-ui/react-select": "^2.2.4",
    "@radix-ui/react-slot": "^1.2.2",
    "@radix-ui/react-tabs": "^1.1.11",
    "@tailwindcss/postcss": "^4.1.5",
    "@tanstack/react-query": "^5.75.5",
    "@tanstack/react-query-devtools": "^5.75.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "embla-carousel-autoplay": "^8.6.0",
    "embla-carousel-fade": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "geist": "^1.4.2",
    "graffle": "8.0.0-next.163",
    "graphql": "^16.11.0",
    "lucide-react": "^0.507.0",
    "next": "15.3.2",
    "next-sitemap": "^4.2.3",
    "payload": "3.38.0",
    "pinyin-pro": "^3.26.0",
    "prism-react-renderer": "^2.4.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.56.2",
    "sharp": "0.34.1",
    "survey-core": "^2.0.8",
    "survey-react-ui": "^2.0.8",
    "tailwind-merge": "^3.2.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@next/eslint-plugin-next": "^15.3.2",
    "@tailwindcss/typography": "^0.5.16",
    "@types/escape-html": "^1.0.4",
    "@types/node": "22.15.14",
    "@types/react": "19.1.3",
    "@types/react-dom": "19.1.3",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "copyfiles": "^2.4.1",
    "eslint": "^9.26.0",
    "eslint-config-next": "^15.3.2",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "tailwindcss": "^4.1.5",
    "tw-animate-css": "^1.2.9",
    "typescript": "5.8.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@tailwindcss/oxide",
      "esbuild",
      "sharp",
      "unrs-resolver"
    ]
  }
}
